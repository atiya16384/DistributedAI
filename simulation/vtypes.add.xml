<add>
    <vTypeDistribution id="mixedDist">
        <!-- Cars -->
        <vType id="car" vClass="passenger" accel="2.6" decel="4.5" length="4.5" minGap="2.5" maxSpeed="50" color="1,1,1"/>

        <!-- Buses -->
        <vType id="bus" vClass="bus" accel="1.2" decel="4.0" length="12" minGap="3.0" maxSpeed="40" color="0,0,1"/>

        <!-- Trucks -->
        <vType id="truck" vClass="truck" accel="1.0" decel="3.5" length="10" minGap="3.0" maxSpeed="30" color="1,1,0"/>

        <!-- Delivery -->
        <vType id="delivery" vClass="delivery" accel="1.5" decel="3.5" length="6" minGap="2.5" maxSpeed="35" color="0.5,0.5,0"/>

        <!-- Taxis -->
        <vType id="taxi" vClass="passenger" accel="2.8" decel="4.5" length="4.3" minGap="2.0" maxSpeed="50" color="1,1,0.2"/>

        <!-- Motorcycles -->
        <vType id="motorcycle" vClass="motorcycle" accel="3.5" decel="4.5" length="2.0" minGap="1.0" maxSpeed="60" color="0.8,0.2,0.8"/>

        <!-- Bicycles -->
        <vType id="bicycle" vClass="bicycle" accel="1.0" decel="2.0" length="1.8" minGap="1.0" maxSpeed="15" color="0.3,0.9,0.3" guiShape="bicycle"/>
    </vTypeDistribution>
</add>
