<routes>
    <vType id="car" accel="1.0" decel="4.5" sigma="0.5" length="5" maxSpeed="20" color="red"/>

    <!-- Define vehicle routes -->
    <route id="route_1" edges="southMiddle___intersectionS intersectionS___intersectionCenter intersectionCenter___intersectionN"/>
    <route id="route_2" edges="westMiddle___intersectionW intersectionW___intersectionCenter intersectionCenter___intersectionE"/>

    <!-- Generate more vehicles -->
    <flow id="flow_1" type="car" route="route_1" begin="0" end="1000" vehsPerHour="1000"/>
    <flow id="flow_2" type="car" route="route_2" begin="0" end="1000" vehsPerHour="1000"/>
</routes>
